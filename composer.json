{
  "name": "chinayin/think-command",
  "description": "thinkphp command library",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": [
    "sdk",
    "library",
    "thinkphp"
  ],
  "support": {
    "source": "https://github.com/chinayin/think-command",
    "issues": "https://github.com/chinayin/think-command/issues"
  },
  "authors": [
    {
      "name": "chinayin",
      "email": "whereismoney@qq.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "chinayin/thinkphp5": "~5.0",
    "aliyunmq/mq-http-sdk": "^1.0",
    "aliyun/aliyun-mns-php-sdk": "^1.1",
    "godruoyi/php-snowflake": "^1.0"
  },
  "require-dev": {
    "eaglewu/swoole-ide-helper": "dev-master",
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "think\\command\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "extra": {
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-ci": "vendor/bin/phpunit --coverage-text",
    "php-cs-fixer": "vendor/bin/php-cs-fixer fix -v",
    "phpstan": "vendor/bin/phpstan analyse",
    "travis": [
      "composer php-cs-fixer",
      "composer phpstan"
    ]
  }
}
